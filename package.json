{
  "name": "concurrent-executor",
  "version": "1.0.1",
  "description": "A TypeScript concurrent task executor with timeout, retry, and execution control",
  "scripts": {
    "prepare": "husky install",
    "build": "rollup  --bundleConfigAsCjs --config rollup.config.js",
    "build:types": "tsc --emitDeclarationOnly --declaration --outDir dist/types src/index.ts",
    "test:unit": "jest",
    "test": "jest --coverage",
    "coveralls": "jest --coverage --coverageReporters=text-lcov | coveralls",
    "lint": "eslint ./src --ext .vue,.js,jsx,.ts,tsx",
    "lint:fix": "eslint --fix ./src --ext .vue,.js,jsx,.ts,tsx",
    "prettier": "prettier -c --write \"**/*.{vue,ts,js,jsx,css,less,scss,json}\""
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "browser": "dist/umd/index.min.js",
  "unpkg": "dist/umd/index.min.js",
  "jsdelivr": "dist/umd/index.min.js",
  "typesVersions": {
    "*": {
      "./concurrentLimit": [
        "./dist/types/concurrentLimit.d.ts"
      ],
      "./concurrentExecutor": [
        "./dist/types/concurrentExecutor.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./concurrentLimit": {
      "import": "./dist/esm/concurrentLimit.js",
      "require": "./dist/cjs/concurrentLimit.js",
      "types": "./dist/types/concurrentLimit.d.ts"
    },
    "./concurrentExecutor": {
      "import": "./dist/esm/concurrentExecutor.js",
      "require": "./dist/cjs/concurrentExecutor.js",
      "types": "./dist/types/concurrentExecutor.d.ts"
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "keywords": [
    "concurrent-executor",
    "async",
    "concurrent",
    "concurrency",
    "promise",
    "promise pool"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chandq/concurrent-executor.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chandq/concurrent-executor/issues"
  },
  "homepage": "https://github.com/chandq/concurrent-executor#readme",
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/eslint-parser": "^7.22.15",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-typescript": "^7.23.2",
    "@commitlint/cli": "^17.8.0",
    "@commitlint/config-conventional": "^17.8.0",
    "@rollup/plugin-commonjs": "^25.0.5",
    "@rollup/plugin-json": "^6.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/jest": "^29.5.5",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.8.0",
    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-import-resolver-webpack": "^0.13.7",
    "eslint-plugin-eslint-plugin": "^5.5.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jest": "^27.4.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-standard": "^5.0.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-canvas-mock": "^2.5.2",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^13.2.2",
    "prettier": "^3.0.3",
    "rollup": "^4.54.0",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-prettier": "^4.1.2",
    "rollup-plugin-subpath-externals": "^3.4.0",
    "standard-version": "^9.5.0",
    "typescript": "5.0.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.[tj]s?(x)": [
      "eslint --fix"
    ],
    "*.vue": [
      "eslint --fix"
    ],
    "*.{css,less,scss,json,md}": [
      "prettier --write"
    ]
  }
}
